{
  "name": "inbev",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf build && babel app --out-dir build",
    "prestart": "npm run migrate:up && rimraf build && babel-node . --out-dir build",
    "start": "node build/index.js",
    "dev": "nodemon --exec babel-node .",
    "migrate": "db-migrate --migrations-dir ./migrations",
    "migrate:up": "db-migrate up",
    "migrate:down": "npm run reset:seed && db-migrate down -c 100",
    "migrate-test:up": "db-migrate up --config database.json -e test",
    "migrate-test:down": "npm run resettest:seed && db-migrate down -c 100 --config database.json -e test",
    "db:seed": "db-migrate up:seed --config database.json",
    "test:seed": "db-migrate up:seed --config database.json  -e test",
    "reset:seed": "db-migrate reset:seed -c 100 --config database.json",
    "resettest:seed": "db-migrate reset:seed -c 100 --config database.json -e test",
    "docker:build": "docker-compose -f 'docker-compose.dev.yml' up -d --build",
    "docker:start": "docker-compose -f 'docker-compose.prod.yml' up --build",
    "docker:stop": "docker-compose -f docker-compose.prod.yml down"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/node": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@babel/runtime": "^7.21.5",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.22"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "helmet": "^4.6.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pg-promise": "^10.11.1",
    "socket.io": "^4.5.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  }
}
