{
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "extends": [
      "airbnb-base"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly",
      "var1": "writable",
      "var2": "readonly",
      "logger": true
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-shadow": ["error", { "allow": ["req", "res", "err", "next"] }],
      "linebreak-style": 0,
      "no-underscore-dangle": 0,
      "object-curly-newline": 0,
      "arrow-parens": 0,
      "no-console": 0,
      "no-unused-vars": "error",
      "no-throw-literal": 0,
      "import/no-extraneous-dependencies": "off",
      "quotes": [
        "error",
        "single",
        {
          "allowTemplateLiterals": true
        }
      ],
      "semi-style": [
        "error",
        "last"
      ],
      "max-len": [
        "error",
        {
          "code": 150
        }
      ],
      "no-irregular-whitespace": "error",
      "no-trailing-spaces": "error",
      "no-multi-spaces": "error",
      "eqeqeq": [
        "error",
        "always"
      ],
      "camelcase": 0,
      "prefer-const": [
        "error",
        {
          "destructuring": "any",
          "ignoreReadBeforeAssign": false
        }
      ]
    }
  }
  